#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

ll tc = 1;

void solve(){
    ll n; cin>>n;
    vector<ll> v(n);
    for(auto &i: v) cin>>i;
    ll ans = v[0];
    for(auto i: v) if(i>ans) ans = i;
    cout<<"Case " <<tc++ <<": ";
    cout<<ans <<'\n';
}


int main(){
    optimize();

    ll t=1;
    cin>>t;
    while(t--)
        solve();
}
