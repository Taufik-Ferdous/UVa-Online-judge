#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

#define fo(i,n) for(ll i=0; i<n; ++i)
#define Fo(i,k,n) for(ll i=k;k<n?i<n:i>=n;i<n?++i:--i)
#define deb(x) cout<<#x <<"=" <<x <<endl
#define deb2(x,y) cout<<#x <<"=" <<x <<" " <<#y <<"=" <<y <<endl
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define all(x) x.begin(), x.end()
#define allr(x) x.rbegin(), x.rend()
#define endl "\n"

typedef pair<ll,ll>      pl;
typedef vector<ll>       vl;

const int mod = 1e9+7;
const ll N = 1e7+10;
//====================================

int main(){
    // optimize();

    // ll t=1;
    // cin>>t;
    while(true){
        ll n; cin>>n;
        if(n==0) break;
        ll a, b; cin>>a >>b;
        while(n--){
            ll aa, bb; cin>>aa >>bb;
            if(aa>a && bb>b) cout<<"NE" <<endl;
            else if(aa>a && bb<b) cout<<"SE" <<endl;
            else if(aa<a && bb<b) cout<<"SO" <<endl;
            else if(aa<a && bb>b) cout<<"NO" <<endl;
            else cout<<"divisa" <<endl;
        }
    }
}
